<!DOCTYPE HTML>
<html>
    <head>
        <title>Search</title>
    </head>
    <body>
        <h1>Search</h1>
        {{ range $i, $e := $ }}
            <h2>{{$e.Person.Name}}</h2>
            <h3>{{$e.Entry.StartString}} to {{$e.Entry.EndString}}</h3>
            <h4>{{$e.Entry.TripReason}}</h4>
            <p>{{$e.Entry.Entry}}</p>
        {{ end }}
    </body>
</html>
